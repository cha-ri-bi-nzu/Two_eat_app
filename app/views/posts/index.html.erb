<h1>TwoEat一覧</h1>

<table>
  <tr>
    <th>皆のTwoEat</th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td><%= post.content %></td>
      <td><%= link_to '開く', post_path(post.id) %></td>
      <td><%= link_to '書き直し', edit_post_path(post.id) %></td>
      <td><%= link_to '消す', post_path(post.id), method: :delete %></td>
    </tr>
  <% end %>
</table>

<br>
<%= link_to 'とぅぶやく', new_post_path %>
